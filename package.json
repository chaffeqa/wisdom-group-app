{
  "name": "wisdom-group-app",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "engines": {
    "node": ">= 10.8.0",
    "npm": ">= 6.4.0",
    "yarn": ">= 1.9.4"
  },
  "scripts": {
    "expo": "expo",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "update-svgs": "svgr --icon --ref --out-dir ./src/client/components --native ./src/client/svgs",
    "lint": "eslint \"./**/*.js\"",
    "format": "prettier --write \"./**/*.js\"",
    "prepare": "patch-package",
    "eject": "expo eject"
  },
  "dependencies": {
    "expo": "^30.0.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "16.5.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "react-native-paper": "^2.0.1",
    "react-native-screens": "^1.0.0-alpha.12",
    "react-native-segmented-control-tab": "^3.3.1",
    "react-native-svg": "^7.0.3",
    "react-native-swipeout": "^2.3.6",
    "react-navigation": "^2.16.0",
    "react-navigation-redux-helpers": "^2.0.6",
    "react-redux": "^5.0.7",
    "react-router-native": "^4.3.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "svgs": "^3.2.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@svgr/cli": "^2.2.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.6.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.3.0",
    "expo-cli": "^2.1.3",
    "husky": "^1.0.0-rc.15",
    "lint-staged": "^7.2.0",
    "patch-package": "^5.1.1",
    "prettier": "1.14.3",
    "remote-redux-devtools": "^0.5.13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
