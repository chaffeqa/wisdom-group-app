{
  "name": "wisdom-group-app",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "engines": {
    "node": ">= 10.8.0",
    "npm": ">= 6.4.0",
    "yarn": ">= 1.9.4"
  },
  "scripts": {
    "expo": "expo",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "update-svgs": "svgr --icon --ref --out-dir ./src/client/components --native ./src/client/svgs",
    "format": "prettier --write \"./**/*.js\"",
    "prepare": "patch-package",
    "eject": "expo eject"
  },
  "dependencies": {
    "expo": "^30.0.0",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "react-native-paper": "^1.11.1",
    "react-native-svg": "^6.5.2",
    "react-router-native": "^4.3.0",
    "svgs": "^3.2.1"
  },
  "devDependencies": {
    "@svgr/cli": "^2.2.0",
    "expo-cli": "^1.1.0-beta.6",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "patch-package": "^5.1.1",
    "prettier": "1.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
